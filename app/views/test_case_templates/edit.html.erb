<h1>Editing test_case_template</h1>
<%= form_for(@test_case_template) do |f| %>
  <table>
    <tr>
      <td>Name <%= f.text_field(:name, :size => @test_case_template.name.size + 10) %></td>
    </tr>
    <tr>
      <td>
      <%= submit_button_green_tag("Update") %>
      <%= cancel_back_button_tag('Cancel') %>
      </td>
    </tr>
  </table>
<% end %>
