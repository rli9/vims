<table border='1' bordercolor='#888888'>
  <% tr_tag do %>
    <%= ths_tag('', 'No.', 'To Do', 'Test Target Instance', 'Change List') %>
    <%= ths_tag_test_data_standard %>
  <% end %>
  <tr>
    <%= td_tag(check_box_tag("test_target_instance_ids[]", @test_target_instance.id)) %>
    <%= td_tag(@test_target_instance_index) %>
    <%= td_tag(link_to(image_tag("test_case.gif", :alt => 'Test Case'), {:controller => 'test_results', :action => "list_test_results", :id => 0, "scope[test_target_instance_id]" => @test_target_instance.id}, :popup => true) +
    link_to(image_tag("edit.gif", :alt => 'edit'), {:controller => 'test_results', :action => "edit", :id => 0, :test_target_instance_id => @test_target_instance.id}, :popup => true) +
    link_to(image_tag("heart.gif", :alt => 'test_report'), {:controller => 'test_results', :action => :report, :id => 0, :test_target_instance_id => @test_target_instance}, :popup => true) +
    link_to(image_tag("indicator.gif", :alt => 'indicator_chart'), {:controller => 'test_results', :action => :indicator, :id => 0, :test_target_instance_id => @test_target_instance}, :popup => true)
    <%= tds_tag(@test_target_instance.name, @test_target_instance.change_list_id) %>
    <%= tds_tag_test_data_standard(@test_target_instance.weighted_test_results) %>
  </tr>
</table>
